void test_ecc_add() {
    printf("Running ecc_add Test...\n");

   ECPoint P = {
        .x = {106941203, 466067982, 438550057, 390471449, 83772637, 320472647, 147247182, 309378531, 14677865, 0},
        .y = {415531087, 405911950, 252905948, 246890892, 45920082, 349355649, 136411145, 205479438, 9595215, 0},
        .is_infinity = 0
    };

    ECPoint Q = {
        .x = {146234469, 52684632, 513982130, 87257946, 41784791, 423629835, 385957795, 184984060, 12206109, 0},
        .y = {488366125, 256433494, 433515110, 100653000, 356946027, 433507502, 68102365, 294805550, 8500433, 0},
        .is_infinity = 0
    };
    u29 result[WORDS] = {0};
    u29 mod[WORDS] = {535425013, 174332635, 444665496, 192778653, 388389189, 518147849, 304619691, 363717891, 15281728, 0};
    u29 mu[WORDS] = {450887704, 490307913, 387807083, 403879883, 291135210, 307268612, 110539282, 24605042, 70628772, 35};
    

    // Perform addition
    ecc_add(&P, &Q, &result_add, mod, mu);
    printf("Addition Result:\n");
    print_array_brr("X", result_add.x,WORDS);
    print_array_brr("Y", result_add.y,WORDS);
}
